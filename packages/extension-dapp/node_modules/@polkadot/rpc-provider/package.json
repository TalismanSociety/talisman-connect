      "default": "./ws/index.js"
    },
    "./ws/errors": {
      "types": "./ws/errors.d.ts",
      "require": "./cjs/ws/errors.js",
      "default": "./ws/errors.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "@polkadot/keyring": "^9.4.1",
    "@polkadot/types": "8.8.1",
    "@polkadot/types-support": "8.8.1",
    "@polkadot/util": "^9.4.1",
    "@polkadot/util-crypto": "^9.4.1",
    "@polkadot/x-fetch": "^9.4.1",
    "@polkadot/x-global": "^9.4.1",
    "@polkadot/x-ws": "^9.4.1",
    "@substrate/connect": "0.7.5",
    "eventemitter3": "^4.0.7",
    "mock-socket": "^9.1.5",
    "nock": "^13.2.6"
  }
