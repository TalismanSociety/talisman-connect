{
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "bugs": "https://github.com/polkadot-js/api/issues",
  "contributors": [],
  "description": "Types generated augmentation",
  "engines": {
    "node": ">=14.0.0"
  },
  "homepage": "https://github.com/polkadot-js/api/tree/master/packages/types-augment#readme",
  "license": "Apache-2.0",
  "maintainers": [],
  "name": "@polkadot/types-augment",
  "repository": {
    "directory": "packages/types-augment",
    "type": "git",
    "url": "https://github.com/polkadot-js/api.git"
  },
  "sideEffects": [
    "./detectPackage.js",
    "./cjs/detectPackage.js"
  ],
  "type": "module",
  "version": "8.8.1",
  "main": "./cjs/index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    "./cjs/package.json": "./cjs/package.json",
    "./cjs/*": "./cjs/*.js",
    ".": {
      "types": "./index.d.ts",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./augment": {
      "types": "./augment.d.ts",
      "require": "./cjs/augment.js",
      "default": "./augment.js"
    },
    "./bundle": {
      "types": "./bundle.d.ts",
      "require": "./cjs/bundle.js",
      "default": "./bundle.js"
    },
    "./detectOther": {
      "types": "./detectOther.d.ts",
      "require": "./cjs/detectOther.js",
      "default": "./detectOther.js"
    },
    "./detectPackage": {
      "types": "./detectPackage.d.ts",
      "require": "./cjs/detectPackage.js",
      "default": "./detectPackage.js"
    },
    "./lookup": {
      "types": "./lookup/index.d.ts",
      "require": "./cjs/lookup/index.js",
      "default": "./lookup/index.js"
    },
    "./lookup/definitions": {
      "types": "./lookup/definitions.d.ts",
      "require": "./cjs/lookup/definitions.js",
      "default": "./lookup/definitions.js"
    },
    "./lookup/kusama": {
      "types": "./lookup/kusama.d.ts",
      "require": "./cjs/lookup/kusama.js",
      "default": "./lookup/kusama.js"
    },
    "./lookup/polkadot": {
      "types": "./lookup/polkadot.d.ts",
      "require": "./cjs/lookup/polkadot.js",
      "default": "./lookup/polkadot.js"
    },
    "./lookup/substrate": {
      "types": "./lookup/substrate.d.ts",
      "require": "./cjs/lookup/substrate.js",
      "default": "./lookup/substrate.js"
    },
    "./lookup/types": {
      "types": "./lookup/types.d.ts",
      "require": "./cjs/lookup/types.js",
      "default": "./lookup/types.js"
    },
    "./lookup/types-kusama": {
      "types": "./lookup/types-kusama.d.ts",
      "require": "./cjs/lookup/types-kusama.js",
      "default": "./lookup/types-kusama.js"
    },
    "./lookup/types-polkadot": {
      "types": "./lookup/types-polkadot.d.ts",
      "require": "./cjs/lookup/types-polkadot.js",
      "default": "./lookup/types-polkadot.js"
    },
    "./lookup/types-substrate": {
      "types": "./lookup/types-substrate.d.ts",
      "require": "./cjs/lookup/types-substrate.js",
      "default": "./lookup/types-substrate.js"
    },
    "./package.json": "./package.json",
    "./packageInfo.js": {
      "types": "./packageInfo.d.ts",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./packageInfo": {
      "types": "./packageInfo.d.ts",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./registry": {
      "types": "./registry/index.d.ts",
      "require": "./cjs/registry/index.js",
      "default": "./registry/index.js"
    },
    "./registry/interfaces": {
      "types": "./registry/interfaces.d.ts",
      "require": "./cjs/registry/interfaces.js",
      "default": "./registry/interfaces.js"
    },
    "./registry/kusama": {
      "types": "./registry/kusama.d.ts",
      "require": "./cjs/registry/kusama.js",
      "default": "./registry/kusama.js"
    },
    "./registry/polkadot": {
      "types": "./registry/polkadot.d.ts",
      "require": "./cjs/registry/polkadot.js",
      "default": "./registry/polkadot.js"
    },
    "./registry/substrate": {
      "types": "./registry/substrate.d.ts",
      "require": "./cjs/registry/substrate.js",
      "default": "./registry/substrate.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "@polkadot/types": "8.8.1",
    "@polkadot/types-codec": "8.8.1",
    "@polkadot/util": "^9.4.1"
  }
}