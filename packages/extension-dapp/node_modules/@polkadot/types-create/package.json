{
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "bugs": "https://github.com/polkadot-js/api/issues",
  "contributors": [],
  "description": "Type creator helpers",
  "engines": {
    "node": ">=14.0.0"
  },
  "homepage": "https://github.com/polkadot-js/api/tree/master/packages/types-create#readme",
  "license": "Apache-2.0",
  "maintainers": [],
  "name": "@polkadot/types-create",
  "repository": {
    "directory": "packages/types-create",
    "type": "git",
    "url": "https://github.com/polkadot-js/api.git"
  },
  "sideEffects": [
    "./detectPackage.js",
    "./cjs/detectPackage.js"
  ],
  "type": "module",
  "version": "8.8.1",
  "main": "./cjs/index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    "./cjs/package.json": "./cjs/package.json",
    "./cjs/*": "./cjs/*.js",
    ".": {
      "types": "./index.d.ts",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./bundle": {
      "types": "./bundle.d.ts",
      "require": "./cjs/bundle.js",
      "default": "./bundle.js"
    },
    "./create": {
      "types": "./create/index.d.ts",
      "require": "./cjs/create/index.js",
      "default": "./create/index.js"
    },
    "./create/class": {
      "types": "./create/class.d.ts",
      "require": "./cjs/create/class.js",
      "default": "./create/class.js"
    },
    "./create/type": {
      "types": "./create/type.d.ts",
      "require": "./cjs/create/type.js",
      "default": "./create/type.js"
    },
    "./detectOther": {
      "types": "./detectOther.d.ts",
      "require": "./cjs/detectOther.js",
      "default": "./detectOther.js"
    },
    "./detectPackage": {
      "types": "./detectPackage.d.ts",
      "require": "./cjs/detectPackage.js",
      "default": "./detectPackage.js"
    },
    "./package.json": "./package.json",
    "./packageInfo.js": {
      "types": "./packageInfo.d.ts",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./packageInfo": {
      "types": "./packageInfo.d.ts",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./types": {
      "types": "./types/index.d.ts",
      "require": "./cjs/types/index.js",
      "default": "./types/index.js"
    },
    "./types/augmentRegistry": {
      "types": "./types/augmentRegistry.d.ts",
      "require": "./cjs/types/augmentRegistry.js",
      "default": "./types/augmentRegistry.js"
    },
    "./types/lookup": {
      "types": "./types/lookup.d.ts",
      "require": "./cjs/types/lookup.js",
      "default": "./types/lookup.js"
    },
    "./types/types": {
      "types": "./types/types.d.ts",
      "require": "./cjs/types/types.js",
      "default": "./types/types.js"
    },
    "./util": {
      "types": "./util/index.d.ts",
      "require": "./cjs/util/index.js",
      "default": "./util/index.js"
    },
    "./util/encodeTypes": {
      "types": "./util/encodeTypes.d.ts",
      "require": "./cjs/util/encodeTypes.js",
      "default": "./util/encodeTypes.js"
    },
    "./util/getTypeDef": {
      "types": "./util/getTypeDef.d.ts",
      "require": "./cjs/util/getTypeDef.js",
      "default": "./util/getTypeDef.js"
    },
    "./util/typeSplit": {
      "types": "./util/typeSplit.d.ts",
      "require": "./cjs/util/typeSplit.js",
      "default": "./util/typeSplit.js"
    },
    "./util/xcm": {
      "types": "./util/xcm.d.ts",
      "require": "./cjs/util/xcm.js",
      "default": "./util/xcm.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "@polkadot/types-codec": "8.8.1",
    "@polkadot/util": "^9.4.1"
  }
}