{
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "bugs": "https://github.com/polkadot-js/api/issues",
  "contributors": [],
  "description": "A JavaScript wrapper for the Polkadot JsonRPC interface",
  "engines": {
    "node": ">=14.0.0"
  },
  "homepage": "https://github.com/polkadot-js/api/tree/master/packages/rpc-core#readme",
  "license": "Apache-2.0",
  "maintainers": [],
  "name": "@polkadot/rpc-core",
  "repository": {
    "directory": "packages/rpc-core",
    "type": "git",
    "url": "https://github.com/polkadot-js/api.git"
  },
  "sideEffects": [
    "./detectPackage.js",
    "./cjs/detectPackage.js"
  ],
  "type": "module",
  "version": "8.8.1",
  "main": "./cjs/index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    "./cjs/package.json": "./cjs/package.json",
    "./cjs/*": "./cjs/*.js",
    ".": {
      "types": "./index.d.ts",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./bundle": {
      "types": "./bundle.d.ts",
      "require": "./cjs/bundle.js",
      "default": "./bundle.js"
    },
    "./detectOther": {
      "types": "./detectOther.d.ts",
      "require": "./cjs/detectOther.js",
      "default": "./detectOther.js"
    },
    "./detectPackage": {
      "types": "./detectPackage.d.ts",
      "require": "./cjs/detectPackage.js",
      "default": "./detectPackage.js"
    },
    "./package.json": "./package.json",
    "./packageInfo.js": {
      "types": "./packageInfo.d.ts",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./packageInfo": {
      "types": "./packageInfo.d.ts",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./types": {
      "types": "./types/index.d.ts",
      "require": "./cjs/types/index.js",
      "default": "./types/index.js"
    },
    "./types/base": {
      "types": "./types/base.d.ts",
      "require": "./cjs/types/base.js",
      "default": "./types/base.js"
    },
    "./types/jsonrpc": {
      "types": "./types/jsonrpc.d.ts",
      "require": "./cjs/types/jsonrpc.js",
      "default": "./types/jsonrpc.js"
    },
    "./util": {
      "types": "./util/index.d.ts",
      "require": "./cjs/util/index.js",
      "default": "./util/index.js"
    },
    "./util/drr": {
      "types": "./util/drr.d.ts",
      "require": "./cjs/util/drr.js",
      "default": "./util/drr.js"
    },
    "./util/memo": {
      "types": "./util/memo.d.ts",
      "require": "./cjs/util/memo.js",
      "default": "./util/memo.js"
    },
    "./util/refCountDelay": {
      "types": "./util/refCountDelay.d.ts",
      "require": "./cjs/util/refCountDelay.js",
      "default": "./util/refCountDelay.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "@polkadot/rpc-augment": "8.8.1",
    "@polkadot/rpc-provider": "8.8.1",
    "@polkadot/types": "8.8.1",
    "@polkadot/util": "^9.4.1",
    "rxjs": "^7.5.5"
  }
}