{
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^build", "^dev"],
      "cache": false
    },
    "release": {
      "cache": false
    },
    "clean": {}
  }
}
