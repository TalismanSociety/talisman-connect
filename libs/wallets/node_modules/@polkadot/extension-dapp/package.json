{
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "bugs": "https://github.com/polkadot-js/extension/issues",
  "contributors": [],
  "description": "Provides an interfaces around the injected globals for ease of access by dapp developers.",
  "homepage": "https://github.com/polkadot-js/extension/tree/master/packages/extension-dapp#readme",
  "license": "Apache-2",
  "maintainers": [],
  "name": "@polkadot/extension-dapp",
  "repository": {
    "directory": "packages/extension-dapp",
    "type": "git",
    "url": "https://github.com/polkadot-js/extension.git"
  },
  "sideEffects": [
    "./detectPackage.js",
    "./detectPackage.cjs"
  ],
  "type": "module",
  "version": "0.42.7",
  "main": "index.cjs",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./bundle": {
      "types": "./bundle.d.ts",
      "require": "./bundle.cjs",
      "default": "./bundle.js"
    },
    "./detectOther": {
      "types": "./detectOther.d.ts",
      "require": "./detectOther.cjs",
      "default": "./detectOther.js"
    },
    "./detectPackage": {
      "types": "./detectPackage.d.ts",
      "require": "./detectPackage.cjs",
      "default": "./detectPackage.js"
    },
    "./package.json": "./package.json",
    "./packageInfo.cjs": "./packageInfo.cjs",
    "./packageInfo.js": "./packageInfo.js",
    "./packageInfo": {
      "types": "./packageInfo.d.ts",
      "require": "./packageInfo.cjs",
      "default": "./packageInfo.js"
    },
    "./util": {
      "types": "./util.d.ts",
      "require": "./util.cjs",
      "default": "./util.js"
    },
    "./wrapBytes": {
      "types": "./wrapBytes.d.ts",
      "require": "./wrapBytes.cjs",
      "default": "./wrapBytes.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "@polkadot/extension-inject": "^0.42.7",
    "@polkadot/util": "^8.3.3",
    "@polkadot/util-crypto": "^8.3.3"
  },
  "peerDependencies": {
    "@polkadot/api": "*",
    "@polkadot/util": "*",
    "@polkadot/util-crypto": "*"
  }
}